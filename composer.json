{
    "name": "renoki-co/php-k8s",
    "description": "Build resources in PHP for any Kubernetes cluster.",
    "keywords": ["laravel", "php"],
    "license": "MIT",
    "homepage": "https://github.com/renoki-co/php-k8s",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/support": "~5.5|~6.0|~7.0",
        "vierbergenlars/php-semver": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\PhpK8s\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\PhpK8s\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "orchestra/database": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
