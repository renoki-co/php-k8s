{
    "name": "renoki-co/php-k8s",
    "description": "Build resources in PHP for any Kubernetes cluster.",
    "keywords": ["laravel", "php", "kubernetes", "k8s", "k3s", "k0s", "cluster", "api"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/php-k8s",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "illuminate/support": "^7.0|^8.0",
        "ratchet/pawl": "^0.3.5",
        "vierbergenlars/php-semver": "^2.1|^3.0"
    },
    "suggest": {
        "ext-yaml": "YAML extension is used to read or generate YAML from PHP K8s internal classes."
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\PhpK8s\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\PhpK8s\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
